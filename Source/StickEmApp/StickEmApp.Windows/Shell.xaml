<Window x:Class="StickEmApp.Windows.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:infrastructure="clr-namespace:StickEmApp.Windows.Infrastructure"
        Title="MainWindow">
    <StackPanel>
        <ContentControl prism:RegionManager.RegionName="{x:Static infrastructure:RegionNames.MainRegion}" />
        <ContentControl prism:RegionManager.RegionName="{x:Static infrastructure:RegionNames.EditVendorRegion}" />
    </StackPanel>
</Window>
