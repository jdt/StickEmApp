<Window x:Class="StickEmApp.Windows.View.VendorView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:view="clr-namespace:StickEmApp.Windows.View"
        Title="VendorView" Height="300" Width="300"> 
    
    <Window.Resources>
        <DataTemplate x:Key="VendorDetailViewTemplate" >
            <view:VendorDetailView></view:VendorDetailView>
        </DataTemplate>
    </Window.Resources>
    <ContentControl Content="{Binding}" DataContext="{Binding VendorViewModel, Source={StaticResource ViewModelFactory}}" ContentTemplate="{StaticResource VendorDetailViewTemplate}"/>
</Window>
