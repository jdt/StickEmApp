<Window x:Class="StickEmApp.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:v="clr-namespace:StickEmApp.Windows.View"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <DataTemplate x:Key="VendorListTemplate" >
            <v:VendorListView></v:VendorListView>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <ContentControl Content="{Binding}" DataContext="{Binding VendorListViewModel, Source={StaticResource ViewModelFactory}}" ContentTemplate="{StaticResource VendorListTemplate}"/>
    </Grid>
</Window>
